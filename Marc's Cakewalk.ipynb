    "def marcsCakewalk(calorie):\n",
    "    k=sorted(calorie,reverse=True)\n",
    "    l=len(calorie)\n",
    "    sum=0\n",
    "    for i in range(l):\n",
    "        sum=sum+(k[i]*(2**i))\n",
    "    print(sum)\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "calorie = list(map(int, input().rstrip().split()))\n",
    "\n",
    "result = marcsCakewalk(calorie)\n"
   ]
